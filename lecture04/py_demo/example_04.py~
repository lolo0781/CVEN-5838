#! /usr/bin/env python

import sys
import getopt

def myExample(args):
    
    print 'argv = ', argv

    try:
        opts, args = getopt.getopt(argv, 'h')

    except:
        print 'Error in user input options'
        exit(0)
    

if __name__=="__main__":

    print '*** Executing Main Routine ***'
    
    # store all entries in the list from 1 on
    args = sys.argv[1:]

    myExample(args)
    
